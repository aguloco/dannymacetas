<script>
  // Función para mostrar la imagen grande al hacer clic
  function showLargeImage(imageSrc) {
      // Crea un elemento de imagen para la imagen agrandada
      var largeImage = document.createElement("img");
      largeImage.src = imageSrc;
      largeImage.classList.add("enlarged-image");

      // Agrega la imagen agrandada al cuerpo del documento
      document.body.appendChild(largeImage);

      // Establece un evento para cerrar la imagen al hacer clic en ella
      largeImage.onclick = function () {
          document.body.removeChild(largeImage);
      };
  }
</script>



