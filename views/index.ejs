<div id="leaflet-map" style="height: 400px;"></div>

<script>
  // Coordenadas de ejemplo ()
  var myLatLng = [-34.755191, -58.402878];

  // Crear un mapa de Leaflet en el contenedor
  var map = L.map("leaflet-map").setView(myLatLng, 15); // Nivel de zoom inicial (15)

  // Agregar un proveedor de mapas (por ejemplo, Mapbox)
  L.tileLayer("https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png").addTo(map);

  // Marcar la ubicación de ejemplo en el mapa
  L.marker(myLatLng).addTo(map);
</script>






